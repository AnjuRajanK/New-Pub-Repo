<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Lendable_Value__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is optional. It automatically populates based on the collateral value multiplied by the collateral advance rate.</description>
    <externalId>false</externalId>
    <formula>IF (LLC_BI__Is_Leased_Asset__c = true, LLC_BI__Value__c, LLC_BI__Value__c *  LLC_BI__Collateral_Type__r.LLC_BI__Advance_Rate__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The collateral value multiplied by the collateral advance rate.</inlineHelpText>
    <label>Lendable Value</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
