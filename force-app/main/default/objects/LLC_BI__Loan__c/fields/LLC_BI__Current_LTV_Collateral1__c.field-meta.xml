<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Current_LTV_Collateral1__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated via a formula. It stores the current LTV based on Collateral1 records.</description>
    <externalId>false</externalId>
    <formula>(LLC_BI__Amount__c + LLC_BI__Total_Prior_Lien_Amount_Collateral1__c) / LLC_BI__Gross_Collateral1_Value__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculates LTV. Advance Rate is not considered in this calculation.</inlineHelpText>
    <label>Current LTV</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
