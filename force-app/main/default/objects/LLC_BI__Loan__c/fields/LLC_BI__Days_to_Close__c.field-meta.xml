<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Days_to_Close__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated via a formula. It stores the number of days from the date the loan was created to the date the loan closed.</description>
    <externalId>false</externalId>
    <formula>ROUND(LLC_BI__Closed_Date__c - DATEVALUE(CreatedDate),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days to Close</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
