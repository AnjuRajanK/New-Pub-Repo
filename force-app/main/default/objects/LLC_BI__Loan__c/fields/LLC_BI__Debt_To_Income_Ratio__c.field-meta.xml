<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Debt_To_Income_Ratio__c</fullName>
    <deprecated>false</deprecated>
    <description>The system uses this formula field to determine and display an applicant’s debt to income ratio.</description>
    <externalId>false</externalId>
    <formula>IF(LLC_BI__Total_Monthly_Income__c &gt; 0 , LLC_BI__Total_Monthly_Debt__c / LLC_BI__Total_Monthly_Income__c, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This field displays an applicant’s debt to income ratio.</inlineHelpText>
    <label>Debt to Income Ratio</label>
    <precision>18</precision>
    <required>false</required>
    <scale>3</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
