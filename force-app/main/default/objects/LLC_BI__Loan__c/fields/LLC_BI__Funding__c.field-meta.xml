<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Funding__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated via formula. It populates based on total amount and the amount disbursed. This field specifies the percentage of the loan amount funded.</description>
    <externalId>false</externalId>
    <formula>IF( LLC_BI__Amount__c&lt;&gt;0, LLC_BI__Total_Disbursed__c / LLC_BI__Amount__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Funding %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
