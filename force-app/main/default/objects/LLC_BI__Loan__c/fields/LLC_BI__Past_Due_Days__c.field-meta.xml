<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Past_Due_Days__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated via formula. It is used to specify the number of days a payment is past due.</description>
    <externalId>false</externalId>
    <formula>IF ((TODAY() - LLC_BI__Next_Payment_Due__c)&gt;=0,(TODAY() - LLC_BI__Next_Payment_Due__c),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Past Due Days</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
