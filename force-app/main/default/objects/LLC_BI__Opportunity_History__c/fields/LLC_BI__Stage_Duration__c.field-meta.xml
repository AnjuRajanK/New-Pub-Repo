<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Stage_Duration__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated with a formula. This formula field specifies the stage duration in days.</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(LLC_BI__End_Time__c), NOW() - LLC_BI__Change_Time__c, LLC_BI__End_Time__c - LLC_BI__Change_Time__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Record Duration Days</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
