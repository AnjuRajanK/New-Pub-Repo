<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Total_Stage_Duration_Excluded_Minutes__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically populated via formula. It identifies the total (sum) number of minutes that have been excluded from the Opportunity History of a particular stage.</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(LLC_BI__Previous_Stage_Duration_Excluded_Minutes__c), LLC_BI__Record_Duration_Excluded_Minutes__c , LLC_BI__Record_Duration_Excluded_Minutes__c + LLC_BI__Previous_Stage_Duration_Excluded_Minutes__c)</formula>
    <label>Total Stage Duration Excluded Minutes</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
