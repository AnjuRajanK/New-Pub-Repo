<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Average_Yield__c</fullName>
    <deprecated>false</deprecated>
    <description>This field indicates the average interest income divided by the average outstanding assets</description>
    <externalId>false</externalId>
    <formula>IF( LLC_BI__Average_Assets__c &gt; 0,  LLC_BI__Interest_Income__c /  LLC_BI__Average_Assets__c , null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Average Yield</label>
    <precision>18</precision>
    <required>false</required>
    <scale>4</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
