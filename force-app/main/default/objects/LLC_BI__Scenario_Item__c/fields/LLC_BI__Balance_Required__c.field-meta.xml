<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Balance_Required__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is automatically calculated via formula. This field stores the annual dollar amount of the minimum required balance.</description>
    <externalId>false</externalId>
    <formula>LLC_BI__Fee_Including_Discounts__c / (LLC_BI__Scenario__r.LLC_BI__Earnings_Credit_Allowance__c / 12)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Balance Required</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
