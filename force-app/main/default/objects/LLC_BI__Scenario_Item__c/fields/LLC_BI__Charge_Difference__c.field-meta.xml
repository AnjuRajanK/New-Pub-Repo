<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Charge_Difference__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is populated automatically via formula. It stores the difference between the competitor&apos;s fee and your own for a bill point.</description>
    <externalId>false</externalId>
    <formula>(LLC_BI__Competitor_Price__c * LLC_BI__Volumes__c) -  LLC_BI__Fee_Including_Discounts__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Charge Difference</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
